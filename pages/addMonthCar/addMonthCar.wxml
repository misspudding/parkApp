<view class="add-month-car-con">
  <view class="add-month-section">
    <view class="add-month-title">停车场</view>
    <view class="add-month-input parkpicker">
      <picker bindchange="bindCasPickerChange" value="{{casIndex0}}" range="{{casArray}}" style='opacity:{{num}}'>
        <view class="weui-input">
          <text>{{casArray[casIndex]}}</text>
        </view>
      </picker>
      <view class="pickerholder" style='background:#f00;' style='display:{{displaySty}}'>
        <text class="plachold">请输入停车场</text>
        <image src='../../resource/images/san.png'></image>
      </view>
    </view>
  </view>
  <view class="add-month-section">
    <view class="add-month-title">月卡号码</view>
    <view class="add-month-input">
      <input type='number' placeholder='选项'></input>
    </view>
  </view>
  <view class="add-month-section">
    <view class="add-month-title">车牌号码</view>
    <view class="add-month-input">
      <input placeholder='请填写车牌号码'></input>
    </view>
  </view>
  <view class="add-month-section">
    <view class="add-month-title">手机号码</view>
    <view class="add-month-input">
      <input type='number' placeholder='请填写月卡预留号码'></input>
    </view>
  </view>
  <view class="btn-add">添加</view>
  <view class="bg"></view>
</view>
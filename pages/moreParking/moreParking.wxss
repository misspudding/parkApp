/* pages/moreParking/moreParking.wxss */

.title {
  height: 160rpx;
  text-align: center;
  font-size: 32rpx;
  color: #666;
  width: 100%;
  line-height: 160rpx;
  position: relative;
  box-shadow: 0 0 2px 2px rgba(102, 102, 102, 0.2);
}

.title .con {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}

.title text{
  float:left;
}

.title text.num {
  font-weight: bold;
  font-size: 46rpx;
  display:block;
  width:92rpx;
  height:69rpx;
  line-height: 69rpx;
  text-align:center;
  margin:45rpx 10rpx 0 10rpx;
  background:url(data:image/png;base64,);
  background-size:contain;
  color:#fff;
  letter-spacing:23rpx;
  box-sizing:border-box;
  padding-left:7rpx;

}

/*tab选项卡*/

.swiper-tab {
  width: 100%;
  border-bottom: 2rpx solid #eaeaea;
  text-align: center;
  line-height: 80rpx;
}

.swiper-tab-list {
  font-size: 30rpx;
  display: inline-block;
  width: 50%;
  color: #777;
}

.on {
  color: #1f84fe;
  border-bottom: 5rpx solid #1f84fe;
}

.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
  background: #fff;
}


.part {
  border-bottom: 1rpx solid #eaeaea;
  padding: 35rpx;
  overflow: hidden;
  position: relative;
}

.part-l .tit {
  display: block;
}

.part-l .tit text {
  float: left;
  color:#383838;
}

.part-l .font {
  display: block;
  clear: both;
}

.part-l .tit {
  font-size: 32rpx;
  line-height: 40rpx;
  float: left;
}

.part-l view.start {
  float: left;
  margin-left: 5px;
}

.part-l view image {
  display: inline-block;
  width: 10px;
  height: 10px;
  margin-right: 3px;
}

.part-l .font {
  font-size: 28rpx;
  color: #999;
  line-height: 40rpx;
  height: 40rpx;
}

.part-l .font text {
  color: #ff9a14;
}

.part-l .tips {
  margin-top: 16rpx;
  line-height: 32rpx;
  font-size: 26rpx;
  color: #999;
  clear: both;
  height: 32rpx;
  display: block;
}

.start {
  display: inline-block;
  width: 90px;
  height: 40rpx;
  position: relative;
  overflow: hidden;
  padding-top: 10rpx;
  box-sizing: content-box;
}

.start image {
  width: 24rpx;
  height: 24rpx;
  float: left;
}

.part-l {
  float: left;
}

.part-r {
  width: 56px;
  height: 56px;
  border-radius: 50%;
  background: #1f84fe;
  position: absolute;
  top: 50%;
  transform: translate(0, -50%);
  right: 35rpx;
  text-align: center;
}

.part-r text {
  display: block;
}

.part-r .font {
  font-size: 20rpx;
  color: #fff;
  white-space: nowrap;
}

.part-r image {
  width: 18px;
  height: 21px;
  margin-top: 6px;
}

/* pages/keyBoard/keyBoard.wxss */
#container{
	width: 100%;
	height: 100%;
	padding-top: 50rpx;
}
#carLicenseBox{
	width: 80%;
	/*height: 100%;*/
	border: 1rpx solid #cccccc;
	border-radius: 10rpx;
	margin: 0 auto;
	padding-bottom: 20rpx;
	overflow: hidden;
	background-color: #ffffff;
}
#carLicenseBox .carLicenseTitle{
	width: 100%;
	height: 70rpx;
	background-color: #478FF1;
	text-align: center;
	line-height: 70rpx;
	color: white;
	font-size: 18rpx;
}
#carLicenseBox .carLicenseMain{
	width: 100%;
	height: 160rpx;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
}
#carLicenseBox .carLicenseMain .ul{
	width: 100%;
	height: 36rpx;
	display: flex;
	flex-direction: row;
	align-items: center;
	justify-content: center;
	
}
#carLicenseBox .carLicenseMain .ul .li{
	width: 26rpx;
	height: 34rpx;
	border: 2rpx solid #cccccc;
	text-align: center;
	line-height: 34rpx;
	float: left;
	margin-right: 8rpx;

}
#carLicenseBox .carLicenseMain .ul .li:last-of-type{
	margin: 0;
}
#carLicenseBox .carLicenseMain .ul .li.active{
	border: 2rpx solid #478FF1;
}
#carLicenseBox .carLicenseMain .ul .li.xinnengyuan{
	background: url(../img/xinnengyuan.png) no-repeat center;
	background-size: 100% 100%;
	border: 2rpx dashed #CCCCCC;
}
#carLicenseBox .submitBtn{
	width: 80%;
	height: 45rpx;
	background-color: #478FF1;
	border: none;
	border-radius: 6rpx;
	text-align: center;
	line-height: 45rpx;
	color: white;
	font-size: 20rpx;
	margin-left: 10%;
}
#keyboardBox{
	width: 100%;
	position: absolute;
	bottom: 0rpx;
	left: 0rpx;
	z-index: 999;
}
#keyboardBox .provienceBox,#keyboardBox .textBox{
	width: 100%;
	background-color: #D0D5D9;
	padding-top: 20rpx;
	padding-bottom: 10rpx;
}
#keyboardBox .provienceBox .ul,#keyboardBox .textBox .ul{
	width: 100%;
	display: flex;
	flex-direction: row;
	justify-content: space-around;
	align-items: center;
	margin-top: 18rpx;
}
#keyboardBox .provienceBox .ul:first-of-type, #keyboardBox .textBox .ul:first-of-type{
	margin-top: 0rpx;
}
#keyboardBox .provienceBox .ul .li,#keyboardBox .textBox .ul .li{
	width: 60rpx;
	height: 80rpx;
	border-radius: 8rpx;
	text-align: center;
	line-height: 80rpx;
	float: left;
	background-color: #fff;
  box-shadow: 0 3rpx 0 rgba(51,51,51,0.5);
}
#keyboardBox .textBox{
	display: block;
}
#keyboardBox .provienceBox .ul .changeContentBtn,#keyboardBox .provienceBox .ul .deleteBtn,#keyboardBox .textBox .ul .changeContentBtn,#keyboardBox .textBox .ul .deleteBtn{
	width: 80rpx;
	height: 80rpx;
	background-color: #ACB3BB;
	text-align: center;
	line-height: 80rpx;
}
#keyboardBox .provienceBox .ul .deleteBtn img,#keyboardBox .textBox .ul .deleteBtn img{
	width: 23rpx;
	height: 16rpx;
	margin: 0rpx;
	margin-top: 12rpx;
}
.deleteBtn image{
  width:60rpx;
  height:45rpx;
  padding-top:20rpx;
  padding-left:2rpx;

}

.swiper-tab .more{
  float:right;
  font-size: 28rpx;
  color:#666;
  height:85rpx;
  line-height:85rpx;
}
.park-record-info{
  border-top:1rpx solid #eaeaea;
  border-bottom:1rpx solid #eaeaea;
  margin-bottom:30rpx;
  background:#fff;
}
.park-record-info .tit{
  margin-left:35rpx;
  padding-right:35rpx;
  height:80rpx;
  line-height:80rpx;
  border-bottom:1rpx solid #eaeaea;
}
.park-record-info .tit .carnum{
  float:left;
  font-size: 36rpx;
  font-family:"黑体";
  color:#383838;
}
.park-record-info .tit .classify{
  float:right;
  color:#666;
  font-size: 26rpx;
}
.park-record-info .detail{
  height:150rpx;
  line-height: 150rpx;
  padding:30rpx 35rpx;
  box-sizing:border-box;
  border-bottom:1rpx solid #eaeaea;
}
.park-record-info .detail image{
  width:85rpx;
  height:85rpx;
  float:left;
}
.park-record-info .detail view{
  float:left;
  overflow:hidden;
  margin-left:20rpx;
}
.park-record-info .detail view .adress{
  font-size: 28rpx;
  color:#333;
  display:block;
  height:43rpx;
  line-height:43rpx;
}
.park-record-info .detail view .time{
  font-size: 26rpx;
  color:#999;
  display:block;
  height:42rpx;
  line-height:42rpx;
}
.park-record-info .detail .money{
  float:right;
  height:100rpx;
  line-height:100rpx;
  font-size: 36rpx;
  color:#383838;
}
.park-record-info .into-btom{
  padding:0 35rpx;
  overflow:hidden;
  height:70rpx;
  line-height:70rpx;
  width:100%;
  box-sizing: border-box;
}
.park-record-info .into-btom .time{
  float:left;
  font-size: 26rpx;
  color:#999;
}
.park-record-info .into-btom .coupon{
  float:right;
  font-size: 26rpx;
  color:#999;
}
.park-record-info .into-btom .time text{
  padding-left:10rpx;
}
.bg{
  position:absolute;
  left:0;
  right:0;
  top:0;
  bottom:0;
  background:#f3f4f5;
  z-index:-9999;
}
/*tab选项卡*/

.swiper-tab {
  text-align: center;
   height:85rpx;
  line-height:85rpx;
  padding:0 35rpx;
  background:#fff;
  box-sizing: border-box;
}

.swiper-tab-list {
 float:left;
  display:block;
  width:137rpx;
  height:57rpx;
  line-height: 57rpx;
  border:1rpx solid #eaeaea;
  margin:14rpx 26rpx 0 0;
  font-size: 28rpx;
  text-align:center;
  border-radius:8rpx;
  color:#666;
}

.on {
  background: #1f84fe;
  border: 1rpx solid #1f84fe;
  color:#fff;
}

.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
  background: #fff;
}

.item-con .item-tit{
 padding:0 35rpx;
  height:90rpx;
  line-height: 90rpx;
  background:#f8f8f8;
  font-size: 32rpx;
  color:#333;
  overflow:hidden;
}
.item-con .item-tit .money{
  float:right;
}
.item-con .item-box{
  border-top:1rpx solid #eaeaea;
  border-bottom:1rpx solid #eaeaea;
  overflow:hidden;
  box-sizing:border-box;
}
.item-box .space{
  width:100%;
  height:1rpx;
}
.item-con .item-box .line{
  margin:0 0 0 35rpx;
  padding:30rpx 35rpx 30rpx 0;
  height:80rpx;
  overflow:hidden;
  border-bottom:1rpx solid #eaeaea;
}
.item-con .item-box .line:last-child{
  border-bottom:none;
}
.item-con .item-box .line image{
  width:63rpx;
  height:63rpx;
  float:left;
  margin-top:3rpx;
}
.item-con .item-box .line view{
  float:left;
  margin-left:20rpx;
}
.item-con .item-box .line view text.tit{
  display:block;
  font-size: 28rpx;
  color:#333;
  height:35rpx;
  line-height: 35rpx;
}
.item-con .item-box .line view text.date{
  display:block;
  font-size: 26rpx;
  height:43rpx;
  line-height:43rpx;
  color:#666;
}
.item-con .item-box .line view text.date text{
  padding-left:30rpx;
}
.item-con .item-box .line .recharge{
  float:right;
  color:#fe6133;
  font-size: 32rpx;
  text-align:right;
  height:80rpx;
  line-height:80rpx;
}